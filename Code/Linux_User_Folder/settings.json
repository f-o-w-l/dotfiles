{
  "workbench.editor.closeEmptyGroups": false,
  "prettier.requireConfig": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.defaultLanguage": "${activeEditorLanguage}",
  "editor.tabSize": 2,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.tsserver.log": "off",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.exclude": {
    "**/node_modules": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "yaml.schemas": {
    "file:///home/fowl/.vscode/extensions/atlassian.atlascode-3.0.10/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "redhat.telemetry.enabled": false,
  "editor.find.cursorMoveOnType": false,
  "atlascode.outputLevel": "debug",
  "atlascode.bitbucket.enabled": true,
  "atlascode.jira.startWorkBranchTemplate.customTemplate": "{{{prefix}}}/{{{issueKey}}}_{{{summary}}}",
  "atlascode.jira.startWorkBranchTemplate.customPrefixes": [
    "ci",
    "doc",
    "feat",
    "fix",
    "perf",
    "style"
  ],
  "explorer.confirmDelete": false,
  "tailwindCSS.lint.recommendedVariantOrder": "error",
  "files.associations": {
    ".env": "plaintext",
    "*.graphql": "graphql",
    "*.vtl": "velocity"
  },
  "omnisharp.useModernNet": false,
  "playwright.reuseBrowser": true,
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "explorer.confirmDragAndDrop": false,
  "git.openRepositoryInParentFolders": "always",
  "[python]": {
    "editor.formatOnType": true
  },
  "python.analysis.typeCheckingMode": "basic",
  "telemetry.telemetryLevel": "crash",
  "editor.formatOnSave": true,
  "editor.snippetSuggestions": "bottom",
  "emmet.showSuggestionsAsSnippets": true,
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[json5]": {},
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "workbench.editor.empty.hint": "hidden",
  "git.autofetch": true,
  "githubIssues.issueBranchTitle": "${user}/#${issueNumber}-${sanitizedIssueTitle}",
  "githubPullRequests.pullBranch": "never",
  "window.zoomLevel": 4,
  "[xml]": {
    "editor.defaultFormatter": "fabianlauer.vs-code-xml-format"
  }
}
